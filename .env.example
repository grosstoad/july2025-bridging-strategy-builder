# Server Configuration
PORT=3100
NODE_ENV=development

# CORS Settings
CORS_ORIGIN=http://localhost:3000,http://localhost:3001,http://localhost:3002,http://localhost:3003,http://localhost:3004

# PropTrack API Authentication
# This should be a Base64 encoded string of your API credentials in the format: base64(api_key:api_secret)
PROPTRACK_AUTH_HEADER=Basic your_base64_encoded_credentials_here

# PropTrack API Keys (for frontend)
VITE_PROPTRACK_API_KEY=your_proptrack_api_key_here
VITE_PROPTRACK_API_SECRET=your_proptrack_api_secret_here

# Optional: API Base URL (if you need to override)
VITE_API_BASE_URL=https://data.proptrack.com

# Australia Post PAC API Authentication
# Official Australia Post PAC API key for postcode/suburb search
VITE_AUSPOST_API_KEY=your_auspost_api_key_here